# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your-access-key-id
AWS_SECRET_ACCESS_KEY=your-secret-access-key

# S3 Configuration
S3_BUCKET_NAME=ppmt-amp-data-bucket
S3_DATA_PATH=market-prices/

# DynamoDB Configuration
DYNAMODB_TABLE_NAME=PPMT-AMP-Prices
DYNAMODB_INDEX_NAME=DateIndex

# Cognito Configuration
COGNITO_USER_POOL_ID=us-east-1_XXXXXXXXX
COGNITO_CLIENT_ID=your-client-id
COGNITO_IDENTITY_POOL_ID=us-east-1:xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx

# Lambda Configuration
LAMBDA_DATA_PROCESSING_FUNCTION=ppmt-amp-process-data
LAMBDA_PRICE_CALCULATION_FUNCTION=ppmt-amp-calculate-price

# App Configuration
APP_ENVIRONMENT=Development
LOG_LEVEL=Debug
